{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "allowJs": true,
    "strict": true,
    "noEmit": true,
    "isolatedModules": true,
    "types": [
      "vinxi/types/client",
      "node",
      "unplugin-icons/types/solid",
    ],
    "paths": {
      "~/*": [
        "./src/*"
      ]
    },
  },
  "exclude": [
    "node_modules",
    ".vinxi",
    ".partykit",
    ".wrangler",
    ".output",
    "dist"
  ]
}
